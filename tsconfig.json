{
  "compilerOptions": {
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "lib": [
      "es2015",
      "dom"
    ],
    "jsx": "react",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": false
  }
}
